{
    "name": "React Extension Template",
    "version": "0.0.1",
    "manifest_version": 3,
    "description": "Created for build well sctructured extension via React library.",
    "action": {
        "default_popup": "popup.html",
        "default_title":"Popup"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [],
            "css": [],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "host_permissions": [
        "https://*/*,http://*/*"
    ]
}